{
  // 应用程序ID
  "AppId": "XSoaCenter",
  // 配置当前应用支持的服务（Soa, Message驱动处理, 定时任务等
  "ServiceHandler": [
    "XFramework.Soa.SoaServiceHandler, XFramework.Soa"
  ],
  // 配置Soa的监听器
  "SoaServiceListener": [
    "soa, XFramework.Soa.Center.Server.Service"
  ],
  "Logging": {
    "IncludeScopes": true,
    "XFramework.Core.Abstractions.Logger.LogProvider": {
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "LogLevel": {
      "Default": "None"
    }
  },

  "AutofacAssemblies": [
    "XFramework.Configuration.Client",
    "XFramework.Soa.Center.Server.Business",
    "XFramework.Soa.Center.Server.Service",
    "XFramework.Soa"
  ]
}
